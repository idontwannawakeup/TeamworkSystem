@page "/teams"

<MudText Class="mt-md-12 mb-4" Typo="@Typo.h3" Align="@Align.Left">Teams</MudText>

<MudTable Items="@(Enumerable.Range(0, 15))" Hover="@true" Outlined="@false">
    <HeaderContent>
        <MudTh>Name</MudTh>
        <MudTh>Specialization</MudTh>
        <MudTh>Leader</MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd DataLabel="Name">Name</MudTd>
        <MudTd DataLabel="Specialization">Specialization</MudTd>
        <MudTd DataLabel="Leader">Leader</MudTd>
    </RowTemplate>
</MudTable>

<MudFab Style="position: fixed; bottom: 20px; right: 20px;"
        Color="Color.Primary"
        Icon="@Icons.Material.Filled.Add"
        Label="Project" />


